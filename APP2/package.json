{
  "name": "APP1",
  "version": "1.0.0",
  "description": "First node.js/tc app",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "parcel": "parcel build src/public/js/main.ts -d dist/public/js",
    "build": "npm-run-all clean lint tsc copy-assets parcel",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "initdb": "ts-node tools/initdb",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "commonjs": "^0.0.1",
    "dotenv": "^5.0.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "mariadb": "^2.5.3",
    "materialize-css": "^1.0.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/materialize-css": "^1.0.8",
    "@types/node": "^14.14.31",
    "@types/shelljs": "^0.8.8",
    "@types/vue": "^2.0.0",
    "fs-extra": "^9.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
}
